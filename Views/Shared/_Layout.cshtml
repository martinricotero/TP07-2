<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TP07</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TP07.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
     <header>
        <div class="navbar">
            <a href="~/Home/Index">
            <img src="~/img/ort.png" alt="Imagen 1" class="navbar-imagen" width="70" height="70" />
        </a>
            <h1 class="navbar-titulo">PreguntadORT</h1>
            <img src="~/img/logo.png" alt="Imagen 2" class="navbar-imagen" width="100" height="80">
        </div>
    </header>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="footer">
        <div class="footer-cont">
            <!-- Contenido del footer -->
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
